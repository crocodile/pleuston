{
  "name": "@oceanprotocol/pleuston",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "node --max_old_space_size=4096 scripts/start.js",
    "build": "node --max_old_space_size=4096 scripts/build.js",
    "lint:js": "eslint ./src/",
    "lint:css": "stylelint ./src/**/*.scss",
    "lint": "run-p --continue-on-error lint:js lint:css",
    "test": "npm run lint && node scripts/test.js --env=jsdom --coverage",
    "coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "requirements-check": "./scripts/check-node-version.sh",
    "preinstall": "npm run requirements-check"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@oceanprotocol/art": "^2.2.0",
    "@oceanprotocol/squid": "0.2.7",
    "aws-amplify": "^1.1.19",
    "azure-storage": "^2.10.2",
    "connected-react-router": "^6.2.1",
    "copy": "^0.3.2",
    "elliptic": "^6.4.1",
    "fetch-download": "^1.1.1",
    "fs-extra": "^7.0.1",
    "history": "^4.7.2",
    "is-url": "^1.2.4",
    "jsonwebtoken": "^8.4.0",
    "object-assign": "^4.1.1",
    "promise": "^8.0.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-blockies": "^1.4.0",
    "react-dom": "^16.7.0",
    "react-modal": "^3.8.1",
    "react-oauth-flow": "^1.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.2.0",
    "react-transition-group": "^2.5.3",
    "react-truncate": "^2.4.0",
    "react-vis": "^1.11.6",
    "react-web3": "^1.2.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.3.0",
    "secp256k1": "^3.6.1",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "^4.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-preset-react-app": "^7.0.0",
    "chalk": "^2.4.2",
    "codacy-coverage": "^3.4.0",
    "css-loader": "^1.0.1",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^4.2.0",
    "eslint": "^5.12.0",
    "eslint-config-oceanprotocol": "^1.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-node": "^8.0.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-safe-parser": "^4.0.1",
    "react-dev-utils": "^7.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-bigchaindb": "^1.2.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "terser-webpack-plugin": "^1.2.1",
    "thread-loader": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "browserslist": {
    "development": [
      "last 2 versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie <= 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
