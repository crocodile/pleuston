{
  "name": "@oceanprotocol/pleuston",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint:js": "eslint ./src/",
    "lint:css": "stylelint ./src/**/*.scss",
    "lint": "run-p --continue-on-error lint:js lint:css",
    "test": "npm run lint && node scripts/test.js --env=jsdom --coverage",
    "coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "requirements-check": "./scripts/check-node-version.sh",
    "preinstall": "npm run requirements-check"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@oceanprotocol/art": "^1.0.2",
    "@oceanprotocol/squid": "^0.0.12",
    "azure-storage": "^2.10.2",
    "connected-react-router": "^5.0.0",
    "copy": "^0.3.2",
    "elliptic": "^6.4.1",
    "eth-crypto": "^1.2.4",
    "ethereumjs-util": "^5.2.0",
    "ethers": "^4.0.0",
    "fetch-download": "^1.1.1",
    "fs-extra": "^7.0.0",
    "history": "^4.7.2",
    "is-url": "^1.2.4",
    "jsonwebtoken": "^8.3.0",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "raf": "^3.4.0",
    "react": "^16.5.2",
    "react-blockies": "^1.3.0",
    "react-dom": "^16.5.2",
    "react-modal": "^3.5.1",
    "react-oauth-flow": "^1.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0",
    "react-transition-group": "^2.5.0",
    "react-truncate": "^2.4.0",
    "react-vis": "^1.11.2",
    "react-web3": "^0.4.5",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0",
    "resolve": "^1.8.1",
    "secp256k1": "^3.5.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@svgr/webpack": "2.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-react-app": "^6.0.0",
    "bfj": "6.1.1",
    "bignumber.js": "^8.0.1",
    "browserslist": "^4.1.1",
    "canvas": "^2.0.1",
    "chalk": "^2.4.1",
    "codacy-coverage": "^3.1.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "dotenv-expand": "^4.2.0",
    "eslint": "^5.6.1",
    "eslint-config-oceanprotocol": "^1.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-node": "^8.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-beta.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.1.0",
    "postcss-safe-parser": "^4.0.1",
    "react-dev-utils": "^6.0.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-bigchaindb": "^1.2.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-manifest-plugin": "^2.0.3",
    "workbox-webpack-plugin": "^3.6.2"
  },
  "browserslist": {
    "development": [
      "last 2 versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie <= 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
